;
; BIND data file for local.lan
;
$TTL	604800
@	IN	SOA	ns.local.lan. root.local.lan. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.local.lan.
@	IN	A	192.168.56.102

ns      IN      A       192.168.56.102
apache  IN      A       192.168.5.8
ldap    IN      A       10.0.2.8
openvpn IN      A       127.0.0.8



